{
	"compilerOptions": {
		"jsx": "react",
		"target": "es2017",
		"module": "commonjs",
		"noEmit": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"**/*.js",
		"**/*.jsx",
		"**/*.json"
	],
	"exclude": [
		"node_modules"
	]
}